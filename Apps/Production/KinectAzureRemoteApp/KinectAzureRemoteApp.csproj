<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
	  <TargetFramework>net48</TargetFramework>
	  <Nullable>enable</Nullable>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <LangVersion>Preview</LangVersion>
    <UseWPF>true</UseWPF>
    <Platforms>x64</Platforms>
	  <BaseOutputPath>$(SolutionDir)build\bin\</BaseOutputPath>
	  <BaseIntermediateOutputPath>$(SolutionDir)build\obj\</BaseIntermediateOutputPath>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.Psi.Audio.Windows" Version="0.17.52.1-beta" />
    <PackageReference Include="Microsoft.Psi.AzureKinect.x64" Version="0.17.52.1-beta" />
    <PackageReference Include="Microsoft.Psi.Imaging.Windows" Version="0.17.52.1-beta" />
    <PackageReference Include="Microsoft.Psi.Runtime" Version="0.17.52.1-beta" />
    <PackageReference Include="VL.Core" Version="2021.4.10" />
  </ItemGroup>

</Project>
